{
  "name": "insite/grumphp-phpcbf",
  "license": "GPL-2.0-or-later",
  "description": "phpcbf task for GrumPHP",
  "require": {
    "phpro/grumphp": "~0.16"
  },
  "config": {
    "platform": {
      "php": "7.1"
    }
  },
  "autoload": {
    "psr-4": {
      "GrumphpPhpcbf\\": "src/"
    }
  },
  "scripts": {
    "lint": "grumphp run --ansi"
  },
  "require-dev": {
    "symfony/var-dumper": "^3.4",
    "squizlabs/php_codesniffer": "^3.5"
  }
}
